#
# Wins
# cmd = cmd /c

# Mac
# cmd =
sep = /
cmd =
objExt= o
appExt= exe
exaExt = exe
objLib = a
cc = cc
ar = ar


rule obj
  command=${cmd} ${cc} -c $in -o $out -I.. -I. -O2  -Wmacro-redefined
rule test
  command=${cmd} ${cc} $in -o $out 
rule lib
  command=${cmd} ${ar} rcs $out $in


#
# core
subninja core${sep}build.ninja
